{
  "name": "rally-backend",
  "version": "1.0.0",
  "description": "Backend for the Rally application with user authentication.",
  "main": "src/app.ts",
  "scripts": {
    "start:dev": "dotenv -e .env.development -- ts-node-dev --respawn --transpile-only src/app.ts",
    "start:prod": "dotenv -e .env.production -- ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/uuid": "^8.3.4",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
